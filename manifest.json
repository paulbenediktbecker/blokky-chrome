{
    "manifest_version": 3,
    "version": "1.0",
    "name": "Blokky Chrome",
    "action": {
      "default_popup": "popup.html"
    },
    "background": {
      "service_worker": "js/background.js",
      "type": "module"
    },
    "permissions": ["storage", "activeTab", "tabs", "alarms"],
    "host_permissions": ["<all_urls>"],
    "web_accessible_resources": [{
      "resources": ["block.html"],
      "matches": ["<all_urls>"]
    }]
  }